# Karakeep Configuration Template
# Copy to .env.karakeep and fill in actual values

# NextAuth Configuration (required)
NEXTAUTH_SECRET=your_random_secret_minimum_32_chars_here

# Meilisearch (required)
MEILI_MASTER_KEY=your_random_meilisearch_master_key_here

# Ollama AI Configuration (optional)
# For AI-powered auto-tagging and summarization
# Make sure to pull the models in Ollama first (e.g., ollama pull gemma3)
OLLAMA_BASE_URL=http://host.docker.internal:11434
INFERENCE_TEXT_MODEL=gemma3
INFERENCE_IMAGE_MODEL=llava
INFERENCE_LANG=en
